programa
{
    funcao inicio() {
        inteiro n1
        inteiro n2
        inteiro soma_do_quadrado

        escreva("Digite o primerio número: ")
        leia(n1)
        escreva("Digite o segundo número: ")
        leia(n2)

        soma_do_quadrado = (n1*n1) + (n2*n2)

        escreva("A soma do quadrado dos núemros é igual à: "+ soma_do_quadrado)
    }
}