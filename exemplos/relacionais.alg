algoritmo "operadores e express√µes RELACIONAIS"

var
    n1, n2: inteiro

inicio
    n1 <- 5
    n2 <- 8

    escreval(n1 > n2) // Maior que
    escreval(n1 < n2) // Menor que
    escreval(n1 = n2) // Igual
    escreval(n1 <> n2) // Diferente
    escreval(n1 >= n2) // Maior igual
    escreval(n1 <= n2) // menor igual

fimalgoritmo