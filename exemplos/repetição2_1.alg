algoritmo "contando de 1 até 10 com REPITA"

var
    cont: inteiro

inicio
// Atribuição de valor
        cont <- 1

// Contando de 1 à 10, onde será repetido o comando para somar o valor de cont até esse valor ser maior que para sair do loop repita.
        repita
            escreval(cont)
            cont <- cont + 1
        ate (cont > 10)

fimalgoritmo