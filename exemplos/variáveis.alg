algoritmo "Variáveis e atribuições"

var
    n1: inteiro
    n2: real
    nome: caractere
    binario: logico

inicio
    n1 <- 5
    n2 <- 6.28
    nome <- "Gabriel"
    binario <- verdadeiro

    //***************************************************************
    escreval(n1)
    escreval(n2)
    escreval(nome)
    escreval(binario)


fimalgoritmo