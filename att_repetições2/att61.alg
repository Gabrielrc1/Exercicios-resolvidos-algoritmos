algoritmo "Atividade 61"
// Descrição : Algoritmo que faz uma contagem de 0 até 30 pulando de 3 em 3 números.

var
    cont: inteiro

inicio
// Atribuição de valores
        cont <- 0

// Contando de 0 até 30
        repita
            escreva(cont," ")
            cont <- cont + 3
        ate (cont > 30)

fimalgoritmo